<?xml version="1.0" encoding="UTF-8"?>
<!--*****************************************Documentatie*****************************************-->
<!-- 28_01_2016 ONDERWERP: aanpassing schema. WAT: Toevoeging vormtype op aggregatieType***-->
<!-- 02_02_2016 ONDERWERP: aanpassing schema. WAT: Toevoeging attribuut group. Elk element gekoppeld aan attribuutgroup***-->
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://www.nationaalarchief.nl/ToPX/v2.3" targetNamespace="http://www.nationaalarchief.nl/ToPX/v2.3" elementFormDefault="qualified" attributeFormDefault="unqualified" version="v2.3">
	<xs:element name="ToPX" type="topxType"/>
	<xs:complexType name="topxType">
		<xs:choice>
			<xs:element name="aggregatie" type="aggregatieType">
				<xs:annotation>
					<xs:documentation>Gegevens over een archief, serie, dossier of record.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="bestand" type="bestandType">
				<xs:annotation>
					<xs:documentation>Gegevens over een bestand, waarbij vorm- en formaat-informatie verplicht meegeleverd dienen te worden.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:choice>
		<xs:attributeGroup ref="optioneelAttribuut"/>
	</xs:complexType>
	<xs:group name="basisEigenschappenGroup">
		<xs:sequence>
			<xs:element name="naam" type="nonEmptyStringTypeAttribuut" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Toepassingsprofiel nr. 4 - Beknopte formeel-inhoudelijke beschrijving (titel) van het record.</xs:documentation>
					<xs:documentation>Optioneel</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="classificatie" type="classificatieType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Toepassingsprofiel nr. 5 - Samenvoeging van archiefbestanddelen tot één nieuw geheel met een eigen identiteit.</xs:documentation>
					<xs:documentation>Optioneel</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="omschrijving" type="string" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Toepassingsprofiel nr. 6 - Vrije tekst met nadere beschrijving van record.</xs:documentation>
					<xs:documentation>Optioneel</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="plaats" type="string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Toepassingsprofiel nr. 7 - Fysieke of virtuele locatie van record.</xs:documentation>
					<xs:documentation>Optioneel</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="dekking" type="dekkingType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Toepassingsprofiel nr. 9 - Nadere aanduiding van tijd en ruimte waarin het record kan worden gepositioneerd.</xs:documentation>
					<xs:documentation>Optioneel</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="externIdentificatiekenmerk" type="externIdentificatiekenmerkType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Toepassingsprofiel nr. 10 - Kenmerken, toegekend aan een record, buiten de huidige beheersomgeving.</xs:documentation>
					<xs:documentation>Optioneel</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="taal" type="taalTypeAttribuut" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Toepassingsprofiel nr. 11 - Taal van een record.</xs:documentation>
					<xs:documentation>Optioneel</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="eventGeschiedenis" type="eventGeschiedenisType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Toepassingsprofiel nr. 12 - Vastlegging van het ontstaan en wijzigingen van een record.</xs:documentation>
					<xs:documentation>Optioneel.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="eventPlan" type="eventPlanType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Toepassingsprofiel nr. 13 - Geeft activiteiten of gebeurtenissen weer die in de toekomst van belang zijn voor het record (wat moet er mee gebeuren, wanneer moet archief worden overgebracht).</xs:documentation>
					<xs:documentation>Optioneel</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="relatie" type="relatieType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Toepassingsprofiel nr. 15 - Element waarmee een relatie gelegd kan worden met andere records (op verschillende aggregatieniveaus) en met andere entiteiten.</xs:documentation>
					<xs:documentation>Optioneel</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="context" type="contextType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Toepassingsprofiel nr. 15C - De verwijzing naar de organisatorische context van het record.</xs:documentation>
					<xs:documentation>Optioneel</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="gebruiksrechten" type="gebruiksrechtenType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Toepassingsprofiel nr. 16 - Voorwaarden voor het gebruik anders dan raadpleging van het record (auteursrechten e.d.).</xs:documentation>
					<xs:documentation>Optioneel</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="vertrouwelijkheid" type="vertrouwelijkheidType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Toepassingsprofiel nr. 17 - Indicatie van niveau van vertrouwelijkheid van de informatie (record).</xs:documentation>
					<xs:documentation>Optioneel</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="openbaarheid" type="openbaarheidType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Toepassingsprofiel nr. 18 - Mogelijke beperkingen aan de raadpleging.</xs:documentation>
					<xs:documentation>Optioneel</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:group>
	<xs:complexType name="aggregatieType">
		<xs:sequence>
			<xs:element name="identificatiekenmerk" type="nonEmptyStringTypeAttribuut">
				<xs:annotation>
					<xs:documentation>Toepassingsprofiel nr. 2 - Uniek kenmerk van een record.</xs:documentation>
					<xs:documentation>Verplicht.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="aggregatieniveau">
				<xs:annotation>
					<xs:documentation>Toepassingsprofiel nr. 3 - Het niveau waarop een record kan worden beschreven.</xs:documentation>
					<xs:documentation>Verplicht.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="aggregatieAggregatieniveauType">
							<xs:attributeGroup ref="optioneelAttribuut"/>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
			<xs:group ref="basisEigenschappenGroup"/>
			<xs:element name="vorm" type="vormType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Toepassingsprofiel nr. 19 - Redactie, verschijningsvorm en structuur van een record.</xs:documentation>
					<xs:documentation>Optioneel.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="integriteit" type="string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Toepassingsprofiel nr. 20 - Volledigheid van een record - juist, volledig, tijdig, geautoriseerd. N.B. Hier wordt de inhoudelijke integriteit bedoeld.</xs:documentation>
					<xs:documentation>Optioneel</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="generiekeMetadata" type="generiekeMetadataType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Algemene metadata waar ToPX niet in voorziet. Dit element dus alleen gebruiken indien voorgaande elementen niet voldoen.</xs:documentation>
					<xs:documentation>Optioneel</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attributeGroup ref="optioneelAttribuut"/>
	</xs:complexType>
	<xs:complexType name="bestandType">
		<xs:sequence>
			<xs:element name="identificatiekenmerk" type="nonEmptyStringTypeAttribuut">
				<xs:annotation>
					<xs:documentation>Toepassingsprofiel nr. 2 - Uniek kenmerk van een record.</xs:documentation>
					<xs:documentation>Verplicht.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="aggregatieniveau">
				<xs:annotation>
					<xs:documentation>Toepassingsprofiel nr. 3 - Het niveau waarop een record kan worden beschreven.</xs:documentation>
					<xs:documentation>Verplicht.</xs:documentation>
					<xs:documentation>Is in het geval van een bestand altijd 'Bestand'.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="bestandAggregatieniveauType">
							<xs:attributeGroup ref="optioneelAttribuut"/>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
			<xs:group ref="basisEigenschappenGroup"/>
			<xs:element name="vorm" type="vormType">
				<xs:annotation>
					<xs:documentation>Toepassingsprofiel nr. 19 - Redactie, verschijningsvorm en structuur van een record.</xs:documentation>
					<xs:documentation>Verplicht.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="integriteit" type="string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Toepassingsprofiel nr. 20 - Volledigheid van een record - juist, volledig, tijdig, geautoriseerd. N.B. Hier wordt de inhoudelijke integriteit bedoeld.</xs:documentation>
					<xs:documentation>Optioneel</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="formaat" type="formaatType" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Toepassingsprofiel nr. 21 - Fysieke/technische aspecten van een record. Verplicht in digitale omgeving.</xs:documentation>
					<xs:documentation>Verplicht op aggregatieniveau "Bestand"</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="generiekeMetadata" type="generiekeMetadataType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Algemene metadata waarin ToPX 2.3 niet in voorziet. Dit element dus alleen gebruiken indien voorgaande elementen niet voldoen.</xs:documentation>
					<xs:documentation>Optioneel</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attributeGroup ref="optioneelAttribuut"/>
	</xs:complexType>
	<xs:complexType name="nonEmptyStringTypeAttribuut">
		<xs:simpleContent>
			<xs:extension base="nonEmptyStringType">
				<xs:attributeGroup ref="optioneelAttribuut"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:simpleType name="nonEmptyStringType">
		<xs:annotation>
			<xs:documentation>Een tekst waarde, die ingevuld dient te zijn (minimale lengte is 1).</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="aggregatieAggregatieniveauType">
		<xs:annotation>
			<xs:documentation>Enumeratie van 4 strings die de verschillende aggregatieniveau's beschrijven: archief, serie, dossier, record.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="Record"/>
			<xs:enumeration value="Dossier"/>
			<xs:enumeration value="Serie"/>
			<xs:enumeration value="Archief"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="bestandAggregatieniveauType">
		<xs:annotation>
			<xs:documentation>Enumeratie met 1 waarde die geldig is voor een bestand: Bestand.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="Bestand"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="classificatieType">
		<xs:sequence>
			<xs:element name="code" type="nonEmptyStringTypeAttribuut">
				<xs:annotation>
					<xs:documentation>Toepassingsprofiel nr. 5.1 - Kenmerk waaronder de record wordt ingedeeld of geklasseerd.</xs:documentation>
					<xs:documentation>Verplicht als het element classificatie wordt opgenomen.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="omschrijving" type="nonEmptyStringTypeAttribuut">
				<xs:annotation>
					<xs:documentation>Toepassingsprofiel nr. 5.2 - Nadere omschrijving van classificatie.</xs:documentation>
					<xs:documentation>Verplicht, als het element classificatie wordt opgenomen.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="bron" type="nonEmptyStringTypeAttribuut">
				<xs:annotation>
					<xs:documentation>Toepassingsprofiel nr. 5.3 - Verwijzing naar het geldende classificatieschema/ordeningsplan.</xs:documentation>
					<xs:documentation>Verplicht, als het element classificatie wordt opgenomen.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="datumOfPeriode" type="datumOfPeriodeType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Toepassingsprofiel nr. 5.4 - Datum of periode wanneer geldig/gebruikt.</xs:documentation>
					<xs:documentation>Verplicht, als het element classificatie wordt opgenomen.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attributeGroup ref="optioneelAttribuut"/>
	</xs:complexType>
	<xs:complexType name="dekkingType">
		<xs:sequence>
			<xs:element name="inTijd" type="periodeType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Toepassingsprofiel nr. 9.1 - Geeft positionering in de tijd aan, waarin iets van kracht is/was dan wel bestond.</xs:documentation>
					<xs:documentation>Optioneel</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="geografischGebied" type="string" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Toepassingsprofiel nr. 9.2 - Geeft ruimtelijke afbakening of locatie van record of werking ervan aan.</xs:documentation>
					<xs:documentation>Optioneel</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attributeGroup ref="optioneelAttribuut"/>
	</xs:complexType>
	<xs:complexType name="periodeType">
		<xs:sequence>
			<xs:element name="begin" type="datumOfJaarType" minOccurs="0"/>
			<xs:element name="eind" type="datumOfJaarType" minOccurs="0"/>
		</xs:sequence>
		<xs:attributeGroup ref="optioneelAttribuut"/>
	</xs:complexType>
	<xs:complexType name="datumOfJaarType">
		<xs:choice>
			<xs:element name="jaar">
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="xs:gYear">
							<xs:attributeGroup ref="optioneelAttribuut"/>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="datum">
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="xs:date">
							<xs:attributeGroup ref="optioneelAttribuut"/>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="datumEnTijd">
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="xs:dateTime">
							<xs:attributeGroup ref="optioneelAttribuut"/>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
		</xs:choice>
		<xs:attributeGroup ref="optioneelAttribuut"/>
	</xs:complexType>
	<xs:simpleType name="dateTimeOrDateType">
		<xs:union memberTypes="xs:dateTime xs:date"/>
	</xs:simpleType>
	<xs:complexType name="externIdentificatiekenmerkType">
		<xs:annotation>
			<xs:documentation>Toepassingsprofiel nr. 10 - Kenmerken, toegekend aan een record, buiten de huidige beheersomgeving.</xs:documentation>
			<xs:documentation>Als het element externIdentificatiekenmerk wordt opgenomen dan is het element nummerBinnenSysteem verplicht.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="kenmerkSysteem" type="string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Toepassingsprofiel nr. 10.1 - Betreft de aanduiding van het systeem, de gegevensverzameling, de andere opslag, de andere organisatie etc. waarin het record een rol heeft gespeeld onder een andere identificatie. Het gaat dus om systemen in de meest brede zin van het woord (niet alleen ICT-systemen) die zich buiten de beheeromgeving bevinden waarin het record beheerd wordt.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="nummerBinnenSysteem" type="nonEmptyStringTypeAttribuut">
				<xs:annotation>
					<xs:documentation>Toepassingsprofiel nr. 10.2 - Betreft de identificatie van het record in het andere systeem. Bijvoorbeeld briefkenmerk van derden, met name bij samenwerking in ketens.</xs:documentation>
					<xs:documentation>Verplicht</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attributeGroup ref="optioneelAttribuut"/>
	</xs:complexType>
	<xs:complexType name="eventGeschiedenisType">
		<xs:sequence>
			<xs:element name="datumOfPeriode" type="datumOfPeriodeType">
				<xs:annotation>
					<xs:documentation>Toepassingsprofiel nr. 12.1 - Datum of periode waarop/-in iets
                        heeft plaatsgevonden.</xs:documentation>
					<xs:documentation>Verplicht</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="type" type="nonEmptyStringTypeAttribuut">
				<xs:annotation>
					<xs:documentation>Toepassingsprofiel nr. 12.2 - Soort activiteit of
                        gebeurtenis.</xs:documentation>
					<xs:documentation>Verplicht</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="beschrijving" type="string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Toepassingsprofiel nr. 12.3 - Nadere omschrijving van de
                        gebeurtenis.</xs:documentation>
					<xs:documentation>Optioneel</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="verantwoordelijkeFunctionaris" type="nonEmptyStringTypeAttribuut">
				<xs:annotation>
					<xs:documentation>Toepassingsprofiel nr. 12.4 - Degene die verantwoordelijk is
                        voor de gebeurtenis of activiteit.</xs:documentation>
					<xs:documentation>Verplicht</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attributeGroup ref="optioneelAttribuut"/>
	</xs:complexType>
	<xs:complexType name="datumOfPeriodeType">
		<xs:choice>
			<xs:element name="datum">
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="xs:date">
							<xs:attributeGroup ref="optioneelAttribuut"/>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="jaar">
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="xs:gYear">
							<xs:attributeGroup ref="optioneelAttribuut"/>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="datumEnTijd">
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="xs:dateTime">
							<xs:attributeGroup ref="optioneelAttribuut"/>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="periode" type="periodeType"/>
		</xs:choice>
		<xs:attributeGroup ref="optioneelAttribuut"/>
	</xs:complexType>
	<xs:complexType name="eventPlanType">
		<xs:sequence>
			<xs:element name="datumOfPeriode" type="datumOfPeriodeType">
				<xs:annotation>
					<xs:documentation>Toepassingsprofiel nr. 13.1 - Datum of periode waarop/-in iets zal plaatsvinden.</xs:documentation>
					<xs:documentation>Verplicht als het element evenPlan is opgenomen.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="type">
				<xs:annotation>
					<xs:documentation>Toepassingsprofiel nr. 13.2 - Soort activiteit of gebeurtenis.</xs:documentation>
					<xs:documentation>Verplicht als het element evenPlan is opgenomen</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="eventPlanTypeWaardenLijstType">
							<xs:attributeGroup ref="optioneelAttribuut"/>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="beschrijving" type="string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Toepassingsprofiel nr. 13.3 - Nadere omschrijving van de gebeurtenis of activiteit.</xs:documentation>
					<xs:documentation>Optioneel</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="aanleiding" type="string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Toepassingsprofiel nr. 13.4 - Indicatie van mechanisme waarop gebeurtenis of actie in gang wordt gezet.</xs:documentation>
					<xs:documentation>Optioneel</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attributeGroup ref="optioneelAttribuut"/>
	</xs:complexType>
	<xs:complexType name="relatieType">
		<xs:sequence>
			<xs:element name="relatieID" type="nonEmptyStringTypeAttribuut">
				<xs:annotation>
					<xs:documentation>Toepassingsprofiel nr. 15.1 - Een link naar de entiteit waarin de relatie beschreven is.</xs:documentation>
					<xs:documentation>Verplicht</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="typeRelatie" type="nonEmptyStringTypeAttribuut">
				<xs:annotation>
					<xs:documentation>Toepassingsprofiel nr. 15.2 - Nadere aanduiding van aard van de relatie.</xs:documentation>
					<xs:documentation>Verplicht</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="datumOfPeriode" type="datumOfPeriodeType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Toepassingsprofiel nr. 15.3 - Datum of periode waarin de relatie bestaat.</xs:documentation>
					<xs:documentation>Optioneel</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attributeGroup ref="optioneelAttribuut"/>
	</xs:complexType>
